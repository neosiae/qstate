{
  "name": "qstate",
  "version": "1.0.0",
  "description": "Save state in query string",
  "type": "module",
  "main": "dist/bundle.js",
  "types": "dist/bundle.d.ts",
  "typesVersions": {
    "*": {
      "react": [
        "src/react/dist/bundle.d.ts"
      ]
    }
  },
  "exports": {
    ".": "./dist/bundle.js",
    "./react": "./src/react/dist/bundle.js"
  },
  "scripts": {
    "_tsc": "tsc",
    "_clear": "rimraf dist/*",
    "_make-bundle": "rollup --config",
    "build": "run-s _clear _tsc _make-bundle",
    "lint": "eslint .",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+github.com/poviolabs/qstate.git"
  },
  "keywords": [
    "save",
    "state",
    "query",
    "string"
  ],
  "author": "Denis Kotlica <denis.kotlica@povio.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/poviolabs/qstate/issues"
  },
  "homepage": "https://github.com/poviolabs/qstate#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/jest": "^28.1.4",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.76.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-esbuild": "^4.9.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "query-string": "^7.1.1"
  },
  "files": [
    "dist/bundle.js",
    "dist/bundle.d.ts",
    "src/react/dist/bundle.js",
    "src/react/dist/bundle.d.ts"
  ]
}
